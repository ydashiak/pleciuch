            "parallax": true,
            "bgImageRadio": false,
            "bgImage": "file:///C:/Users/OhneArbeitStummKuche/AppData/Local/Mobirise.com/Mobirise/projectData/mbr-1-2000x1333.jpg",
            "bgColor": "#28324e",
